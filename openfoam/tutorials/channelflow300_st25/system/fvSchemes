FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss cubic;
}

divSchemes
{
    default         Gauss linear; //none;
}


laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default         cubic;
}

snGradSchemes
{
    default         uncorrected;
}

wallDist
{
    method meshWave;
}


